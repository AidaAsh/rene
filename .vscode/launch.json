{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "Django",
        "type": "python",
        "request": "launch",
        "stopOnEntry": false,
        "python": "${workspaceRoot}/venv/Scripts/python",
        "program": "${workspaceRoot}/manage.py",
          "args": [
              "runserver",
              "8000"
          ],
          "env": {
              "DJANGO_DB_NAME": "db",
              "DJANGO_DB_HOST": "172.17.0.2",
              "DJANGO_DB_PORT": "8000",
              "DJANGO_DEBUG": "True",
              "DJANGO_SETTINGS_MODULE": "settings"
          }
      },
    ]
  }